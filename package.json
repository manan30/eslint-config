{
  "name": "@elegante/eslint-config",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "lint:check": "turbo run lint:check",
    "lint:fix": "turbo run lint:fix",
    "format:check": "turbo run format:check",
    "format:fix": "turbo run format:fix"
  },
  "devDependencies": {
    "eslint-config-elegante-base": "workspace:*",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.1.0"
}
